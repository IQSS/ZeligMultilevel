URL: http://projects.iq.harvard.edu/zelig
Date: 2016-02-17
Depends:
    Zelig,
    dplyr,
    lme4
Collate:
    "model-mixed.R"
    "model-gamma-mixed.R"
    "model-ls-mixed.R"
    "model-poisson-mixed.R"
    "utils.R"

