URL: http://projects.iq.harvard.edu/zelig
Date: 2016-08-21
Depends:
    arm,
    Zelig,
    dplyr,
    lme4
Collate:
    "model-mixed.R"
    "model-gamma-mixed.R"
    "model-ls-mixed.R"
    "model-poisson-mixed.R"
    "model-logit-mixed.R"
    "model-probit-mixed.R"
    "create-json.R"

